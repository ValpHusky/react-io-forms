{
    "presets": ["@babel/preset-react", "@babel/preset-env"],
    "plugins": [
        ["@babel/plugin-proposal-class-properties"],
        ["module-resolver", {
            "root": ["./src"],
            "alias": {
                "@default": "./@default",
                "root": "./",
                "core": "./core",
                "providers": "./providers"
            }
          }]
    ]
}